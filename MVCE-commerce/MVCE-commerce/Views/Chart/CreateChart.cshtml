@model MVCE_commerce.DTO.ChartRequestModel
<div >
    <form asp-action="CreateChart">
        <label>
            Quantity
        </label>
        <input type="number" name="Quantity" required="" />
      
        <select asp-for="ProductNo" asp-items="ViewBag.Products">
            <option selected disabled>--Select Products--</option>
        </select>
        <div>
            <input type="submit" value="submit" />
        </div>
    </form>
    <div>
        @ViewBag.Message
    </div>
    <div>
        <a asp-action="CreateChart">Your list</a>
    </div>
</div>


